{
	"compilerOptions": {
		"outDir": "./dist",
		"baseUrl": "./dist",
		"rootDir": "./src",
		"target": "es5",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	},
	"include": ["./src/**/*"],
	"exclude": []
}
